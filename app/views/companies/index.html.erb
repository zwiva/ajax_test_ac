<p id="notice"><%= notice %></p>

<h1>Companies</h1>

<%= form_with(model: @company) do |form| %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>

<table>
 <thead>
   <tr>
     <th>Content</th>
     <th colspan="3"></th>
   </tr>
 </thead>

 <tbody id="companies">

<%= render @companies %>
   
 </tbody>
</table>

